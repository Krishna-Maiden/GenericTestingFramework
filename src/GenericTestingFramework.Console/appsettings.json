{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "GenericTestingFramework": "Information",
      "Microsoft": "Warning"
    }
  },
  "LLM": {
    "ApiKey": "demo-mode-no-key-required",
    "Model": "mock-model",
    "MaxTokens": 4000,
    "Temperature": 0.7,
    "TopP": 1.0,
    "TimeoutSeconds": 300,
    "MaxRetries": 3,
    "RetryDelayMs": 1000,
    "SystemPrompt": "Mock LLM service for demonstration purposes",
    "EnableCaching": false,
    "CacheDurationMinutes": 60,
    "EnableLogging": true,
    "RateLimit": {
      "RequestsPerMinute": 60,
      "TokensPerMinute": 90000,
      "Enabled": false
    }
  },
  "UITestConfiguration": {
    "BaseUrl": "https://demo.example.com",
    "Headless": true,
    "WindowSize": "1920,1080",
    "DefaultTimeoutSeconds": 30,
    "ImplicitWaitSeconds": 10,
    "MaxParallelSessions": 1,
    "ScreenshotPath": "./screenshots",
    "CaptureScreenshotOnFailure": false,
    "AdditionalOptions": {}
  },
  "APITestConfiguration": {
    "DefaultTimeoutSeconds": 30,
    "MaxConcurrentRequests": 3,
    "DefaultHeaders": {
      "User-Agent": "GenericTestingFramework-Console/1.0.0"
    }
  }
}